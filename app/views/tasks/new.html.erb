<h1>Create</h1>
<%= form_with model: @task do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :details %>
  <%= f.text_field :details %>
  <%= f.label :completed, "Done?" %>
  <%= f.check_box :completed %>
  <%= f.submit %>
<% end %>
<p></p>
<%= link_to "Back", tasks_path %>
